<% provide(:title, t(".home_title")) %>
<div class="center jumbotron">
  <h1><%= t(".home_title") %></h1>
  <h2>
    <%= t(".description", link_to_rails: link_to( t("link_to_rails_name"), t(".link_to_rails"))).html_safe %>
  </h2>
  <%= link_to t(".link_to_signup"), signup_path, class: "btn btn-lg btn-primary" %>
</div>
<%= link_to image_tag("rails.png", alt: "Rails logo"), "http://rubyonrails.org/" %>
<%= image_tag("kitten.jpg", alt: "kitten", id:"kitten") %>
